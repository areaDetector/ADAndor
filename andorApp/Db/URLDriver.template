# URL records.  These are not PINI=YES because the URLSelect record is.
record(waveform, "$(P)$(R)URL1")
{
    field(DTYP, "asynOctetWrite")
    field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))URL_NAME")
    field(FTVL, "CHAR")
    field(NELM, "256")
}

