# mbbo record to select 1 of the 8 URLs. 
# The strings can be changed at run-time and are in save/restore
record (mbbo, "$(P)$(R)URLSelect")
{
    field(PINI, "YES")
    field(ZRST, "URL1")
    field(ZRVL, "1")
    field(ONST, "URL2")
    field(ONVL, "2")
    field(TWST, "URL3")
    field(TWVL, "3")
    field(THST, "URL4")
    field(THVL, "4")
    field(FRST, "URL5")
    field(FRVL, "5")
    field(FVST, "URL6")
    field(FVVL, "6")
    field(SXST, "URL7")
    field(SXVL, "7")
    field(SVST, "URL8")
    field(SVVL, "8")
    field(EIST, "URL9")
    field(SVVL, "8")
    field(NIST, "URL10")
    field(NIVL, "8")
    field(FLNK, "$(P)$(R)URLSeq.PROC PP MS")
}

record(seq, "$(P)$(R)URLSeq")
{
    field(SELL, "$(P)$(R)URLSelect.RVAL NPP")
    field(SELM, "Specified")
    field(DOL1, "1")
    field(LNK1, "$(P)$(R)URL1.PROC PP MS")
    field(DOL2, "1")
